    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "bingher\\obs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "bingher\\obs\\tests\\": "tests/"
        }
    },
